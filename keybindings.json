[
    
    // NAV --------------------------------------------------------------------

    // focus tab
    {
        "key": "ctrl+alt+right",
        "command": "workbench.action.nextEditorInGroup"
    },
    {
        "key": "ctrl+alt+left",
        "command": "workbench.action.previousEditorInGroup"
    },
    // move tab
    {
        "key": "ctrl+shift+alt+right",
        "command": "workbench.action.moveEditorRightInGroup"
    },
    {
        "key": "ctrl+shift+alt+left",
        "command": "workbench.action.moveEditorLeftInGroup"
    },
    // move tab to group
    {
        "key": "ctrl+alt+enter ctrl+alt+]",
        "command": "workbench.action.moveEditorToNextGroup"
    },
    {
        "key": "ctrl+alt+enter ctrl+alt+[",
        "command": "workbench.action.moveEditorToPreviousGroup"
    },
    // focus group
    {
        "key": "ctrl+alt+]",
        "command": "workbench.action.focusNextGroup"
    },
    {
        "key": "ctrl+alt+[",
        "command": "workbench.action.focusPreviousGroup"
    },
    // move group
    {
        "key": "ctrl+shift+alt+]",
        "command": "workbench.action.moveActiveEditorGroupRight"
    },
    {
        "key": "ctrl+shift+alt+[",
        "command": "workbench.action.moveActiveEditorGroupLeft"
    },    
    // toggle maximize group
    {
        "key": "ctrl+alt+L",
        "command": "workbench.action.toggleMaximizeEditorGroup"
    },

    // RUN --------------------------------------------------------------------
    
    // run line
    {
        "key": "cmd+enter",
        "command": "workbench.action.positronConsole.executeCode",
        "when": "editorTextFocus && !notebookEditorFocused && !positronNotebookCellEditorFocused"
    },

    // EDITOR -------------------------------------------------------------------

    // delete line
    {
        "key": "cmd+shift+backspace",
        "command": "editor.action.deleteLines"
    },
    // insert line above/below
    {
        "key": "alt+up",
        "command": "editor.action.insertLineBefore",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "editor.action.insertLineAfter",
        "when": "editorTextFocus && !editorReadonly"
    },
    // move line above/below
    {
        "key": "cmd+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    // copy line above/below
    {
        "key": "shift+cmd+up",
        "command": "editor.action.copyLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+cmd+down",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    // remove brackets
    {
        "key": "alt+cmd+backspace",
        "command": "-editor.action.removeBrackets",
        "when": "editorTextFocus"
    },

    // LANG R -----------------------------------------------------------------

    // insert <-
    {
        "key": "shift+cmd+-",
        "command": "r.insertLeftAssignment",
        "when": "editorTextFocus && editorLangId == 'r' || editorTextFocus && quarto.document.languageId == 'r'"
    },
    {
        "key": "shift+cmd+-",
        "command": "r.insertLeftAssignmentConsole",
        "when": "positronConsoleFocused && editorLangId == 'r'"
    },
    // insert |>
    {
        "key": "shift+cmd+m",
        "command": "r.insertPipe",
        "when": "editorTextFocus && editorLangId == 'r' || editorTextFocus && quarto.document.languageId == 'r'"
    },
    {
        "key": "shift+cmd+m",
        "command": "r.insertPipe",
        "when": "editorTextFocus && editorLangId == 'r' || editorTextFocus && quarto.document.languageId == 'r'"
    },
    // insert section
    {
        "key": "cmd+i cmd+s",
        "command": "r.insertSection",
        "when": "editorLangId == 'r'"
    },
    
    
    // REMOVE CLUTTER ---------------------------------------------------------

    {
        "key": "ctrl+enter",
        "command": "-workbench.action.positronConsole.executeCode",
        "when": "editorTextFocus && !notebookEditorFocused && !positronNotebookCellEditorFocused"
    },
    {
        "key": "alt+cmd+end",
        "command": "-workbench.action.positronConsole.executeCodeAfterCursor",
        "when": "positronConsoleFocused || editorTextFocus && !notebookEditorFocused && !positronNotebookCellEditorFocused"
    },
    {
        "key": "cmd+] cmd+backspace",
        "command": "editor.action.removeBrackets",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+-",
        "command": "-r.insertLeftAssignmentConsole",
        "when": "positronConsoleFocused && editorLangId == 'r'"
    },
    {
        "key": "alt+-",
        "command": "-r.insertLeftAssignment",
        "when": "editorTextFocus && editorLangId == 'r' || editorTextFocus && quarto.document.languageId == 'r'"
    },
    {
        "key": "shift+cmd+t",
        "command": "-r.packageTestExplorer",
        "when": "isRPackage"
    },
    {
        "key": "shift+cmd+b",
        "command": "-r.packageInstall",
        "when": "isRPackage"
    },
    {
        "key": "shift+cmd+l",
        "command": "-r.packageLoad",
        "when": "isRPackage"
    },
    {
        "key": "shift+cmd+e",
        "command": "-r.packageCheck",
        "when": "isRPackage"
    },
    {
        "key": "shift+cmd+d",
        "command": "-r.packageDocument",
        "when": "isRPackage"
    },
    {
        "key": "cmd+k h",
        "command": "-r.insertSection",
        "when": "editorLangId == 'r'"
    },
    {
        "key": "shift+cmd+-",
        "command": "-workbench.action.zoomOut"
    },
    {
        "key": "alt+cmd+home",
        "command": "-workbench.action.positronConsole.executeCodeBeforeCursor",
        "when": "positronConsoleFocused || editorTextFocus && !notebookEditorFocused && !positronNotebookCellEditorFocused"
    },
    {
        "key": "ctrl+alt+right",
        "command": "-quickInput.acceptInBackground",
        "when": "cursorAtEndOfQuickInputBox && inQuickInput && quickInputType == 'quickPick' || inQuickInput && !inputFocus && quickInputType == 'quickPick'"
    },
    {
        "key": "cmd+k alt+cmd+right",
        "command": "-workbench.action.nextEditorInGroup"
    },
    {
        "key": "cmd+k alt+cmd+left",
        "command": "-workbench.action.previousEditorInGroup"
    },
    {
        "key": "ctrl+alt+left",
        "command": "-cursorWordPartLeft",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+alt+right",
        "command": "-cursorWordPartRight",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+shift+alt+right",
        "command": "-cursorWordPartRightSelect",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+shift+alt+left",
        "command": "-cursorWordPartLeftSelect",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+cmd+right",
        "command": "-workbench.action.moveEditorToNextGroup"
    },
    {
        "key": "ctrl+cmd+left",
        "command": "-workbench.action.moveEditorToPreviousGroup"
    },
    {
        "key": "shift+cmd+[",
        "command": "-workbench.action.previousEditor"
    },
    {
        "key": "shift+cmd+[",
        "command": "-workbench.action.terminal.focusPrevious",
        "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
    },
    {
        "key": "shift+cmd+[",
        "command": "-workbench.action.debug.prevConsole",
        "when": "inDebugRepl"
    },
    {
        "key": "cmd+k shift+cmd+right",
        "command": "-workbench.action.moveEditorRightInGroup"
    },
    {
        "key": "cmd+k shift+cmd+left",
        "command": "-workbench.action.moveEditorLeftInGroup"
    },
    {
        "key": "ctrl+shift+alt+[",
        "command": "-jupyter.extendSelectionByCellAbove",
        "when": "editorTextFocus && jupyter.hascodecells && !jupyter.webExtension && !notebookEditorFocused"
    },
    {
        "key": "ctrl+shift+alt+]",
        "command": "-jupyter.extendSelectionByCellBelow",
        "when": "editorTextFocus && jupyter.hascodecells && !jupyter.webExtension && !notebookEditorFocused"
    },
    {
        "key": "cmd+k right",
        "command": "-workbench.action.moveActiveEditorGroupRight"
    },
    {
        "key": "cmd+k left",
        "command": "-workbench.action.moveActiveEditorGroupLeft"
    },
    {
        "key": "shift+cmd+k",
        "command": "-editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "shift+cmd+enter",
        "command": "-editor.action.insertLineBefore",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+enter",
        "command": "-editor.action.insertLineAfter",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+enter",
        "command": "-editor.action.insertLineAfter",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+k cmd+m",
        "command": "-workbench.action.toggleMaximizeEditorGroup",
        "when": "editorPartMaximizedEditorGroup || editorPartMultipleEditorGroups"
    },
    {
        "key": "alt+up",
        "command": "-editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "-editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+down",
        "command": "-editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
]